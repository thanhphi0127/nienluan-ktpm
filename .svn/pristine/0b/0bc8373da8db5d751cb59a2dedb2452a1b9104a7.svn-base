@import "manipulation.css";
@import "../common_header.css";

/*-------------------------------------Genarally--------------------------------------------*/
a {
	text-decoration:none;
}
a:hover {
	color:blue;
}
body {
	color:black;
}

ul {
	list-style:none;
	font-weight:13px;
	
}

.tasklist ul, .reportlist ul {
	line-height:30px;
}
/*------------------------------------------------------------------------------------------*/

#calendar{
	width:100%;
	z-index:400;
}


section.reportlist {
	
}

section.view {
	border:1px solid #D1D1D1;
	min-height:368px;
	margin:0px 0px 5px 0px;
	overflow:hidden;
	background:#F4F4F4;
	padding:10px;
}

section.view .advance{
	float:left;
	width:100%;
	
}

section.view .advance .search{
	float:left;
	width:100%;
	margin:0px 0px 10px 10px;
}
div.span_title{
	padding:5px 30px 5px 0px;
	width:130px;
	float:left;
	font-weight:700;
}

section.view .advance .search form > * {
	margin:4px;
} 
section.view .advance .search form input[type=submit]{
	margin:-44px 90px 0px 0px;
}

form input[type=number], form input[type=date] {
margin: 8px 98px 0px 67px;
width: 156px;
}

section.view .advance .search form input {
	padding:3px;
}

section.view .advance .search form select {
	text-align:center;
	padding:3px;
}


.center{
	text-align:center;
}


section.manipulation, #deltetask{
	display:none;
	
}

section.manipulation {
	position: absolute;
	top: 68px;
	left: 0%;
	z-index: 7;
}

form select[multiple] {
	height:154px;
}
section.manipulation  img{
	position: absolute;
	top: 0;
	right: 0%;
}

#iframe_login div.iframe_login_up {
	background:#FFF;
	border-bottom:5px solid #0080FF;
	text-align:center;
	width:100%;
	height:50px;
	margin:-40px 0px 0px 0px;
}

#iframe_login div.iframe_login_up  img{
	margin:2px auto;
}



.hide{
	display:none;
}

.hideinput {
	background:none;
	border:none;
}

.w60 {
	width:60px;
}
.w50p {
	width:50%;;
}
.hide{
	display:none;
}
.check {
	float:none;
}

section.manipulation div.form form {
width: 50%;
position: absolute;
top: 10%;
left: 22%;
right: 35%;
}

section.manipulation table tr td {
	width:50%;
	padding:10px;
	vertical-align:top;
}

section.manipulation img.manipulation {	
	display:none;
}

section.manipulation div.form form fieldset{
	height:181px;
}

 div.list {
width: 100%;
text-align: center;
padding: 0px;
background: #FFF;
}

textarea {
	height:80px;
}

div.bar_tilehoanthanh, div.bar_tilehoanthanh2 {
	width:100%;
	border:1px solid #ccc;
	height:40px;
	margin: 0px 0px 0px 7px;
}
div.bar_tilehoanthanh  > div, div.bar_tilehoanthanh2 > div{
	background:green;
	height:40px;
	width:0px;
	margin:0px;
}

div.list.tasklist table  {
	font-size:12px;
}


div.list.tasklist table tr td:nth-child(1){
	width:3%;
}
div.list.tasklist table tr td:nth-child(2){
	width:10%;
}
div.list.tasklist table tr td:nth-child(3){
	width:10%;
}
div.list.tasklist table tr td:nth-child(4), div.list.tasklist table tr td:nth-child(5){
	width:7%;
}
div.list.tasklist table tr td:nth-child(6) {
	width:3%;
}
div.list.tasklist table tr td:nth-child(7) {
	width:16%;
}
div.list.tasklist table tr td:nth-child(8) {
	width:3%;
}
div.list.tasklist table tr td:nth-child(9) {
	width:15%;
}
div.list.tasklist table tr td:nth-child(10), div.list.tasklist table tr td:nth-child(11), div.list.tasklist table tr td:nth-child(12){
	width:6%;
}
div.list.tasklist table tr td:last-child{
	width:10%;
}


div.list.tasklist table tr td:nth-child(1){
	width:5px;
}

.error {
	color:red;
	font-style:italic;
}

.form label span {
	float:none;
}
span.manipulation img {
	width:30px;
	height:30px;
	cursor:pointer;
	margin:4px;
}


section.advance table tr td{
	padding:8px;
	
}

section.advance table tr td select{
	width:200px;
	
}

section.advance table tr td select option{
	text-align:center;
}