
/* button Add - Modify - Delete  :a, span*/
.btnAMD_TIM {
	margin:2px auto;
	text-align:center;
	background:-webkit-gradient(linear, left top, left bottom, color-stop(0.05, #9845c7), color-stop(1, #9e3162));
	background:-moz-linear-gradient(top, #9845c7 5%, #9e3162 100%);
	background:-webkit-linear-gradient(top, #9845c7 5%, #9e3162 100%);
	background:-o-linear-gradient(top, #9845c7 5%, #9e3162 100%);
	background:-ms-linear-gradient(top, #9845c7 5%, #9e3162 100%);
	background:linear-gradient(to bottom, #9845c7 5%, #9e3162 100%);
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#9845c7', endColorstr='#9e3162',GradientType=0);
	background-color:#9845c7;
	-moz-border-radius:28px;
	-webkit-border-radius:28px;
	border-radius:28px;
	border:1px solid #b634c2;
	display:inline-block;
	cursor:pointer;
	color:#ffffff;
	font-family:arial;
	font-size:14px;
	padding:0px 9px;
	text-decoration:none;
	text-shadow:0px 1px 0px #2f6627;
}
.btnAMD_TIM:hover {
	background:-webkit-gradient(linear, left top, left bottom, color-stop(0.05, #9e3162), color-stop(1, #9845c7));
	background:-moz-linear-gradient(top, #9e3162 5%, #9845c7 100%);
	background:-webkit-linear-gradient(top, #9e3162 5%, #9845c7 100%);
	background:-o-linear-gradient(top, #9e3162 5%, #9845c7 100%);
	background:-ms-linear-gradient(top, #9e3162 5%, #9845c7 100%);
	background:linear-gradient(to bottom, #9e3162 5%, #9845c7 100%);
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#9e3162', endColorstr='#9845c7',GradientType=0);
	background-color:#9e3162;
}
.btnAMD_TIM:active {
	position:relative;
	top:1px;
}

/* button Find :input type=submit / button*/


.btnFind {
	text-align:center;
  background: #3498db;
  background-image: -webkit-linear-gradient(top, #3498db, #2980b9);
  background-image: -moz-linear-gradient(top, #3498db, #2980b9);
  background-image: -ms-linear-gradient(top, #3498db, #2980b9);
  background-image: -o-linear-gradient(top, #3498db, #2980b9);
  background-image: linear-gradient(to bottom, #3498db, #2980b9);
  -webkit-border-radius: 28;
  -moz-border-radius: 28;
  border-radius: 28px;
  font-family: Arial;
  color: #ffffff;
  font-size: 13px;
  padding: 10px 20px 10px 20px;
  text-decoration: none;
  width:84px;
}

.btnFind:hover {
  background: #3cb0fd;
  background-image: -webkit-linear-gradient(top, #3cb0fd, #3498db);
  background-image: -moz-linear-gradient(top, #3cb0fd, #3498db);
  background-image: -ms-linear-gradient(top, #3cb0fd, #3498db);
  background-image: -o-linear-gradient(top, #3cb0fd, #3498db);
  background-image: linear-gradient(to bottom, #3cb0fd, #3498db);
  text-decoration: none;
}


